{
    "Type": "Task",
    "Resource": "arn:aws:states:::dynamodb:updateItem",
    "ResultPath": "$.updateOrderStatusOutput",
    "Parameters": {
        "TableName": "Orders",
        "Key": {
            "orderHash": {
                "S.$": "$.orderHash"
            }
        },
        "UpdateExpression": "SET orderStatus = :orderStatus",
        "ExpressionAttributeValues": {
            ":orderStatus": {
                "S.$": "$.prevCheckOrderOutput.Payload.orderStatus"
            }
        }
    }
}